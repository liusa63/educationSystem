<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.icss.entity.Course" table="course">
		<id name="tcId" column="tc_id" type="integer">
		<generator class="native"></generator>
		</id>
		<!-- 普通属性 -->
		<property name="coId" column="co_id" type="integer"></property>
		<property name="roId" column="ro_id" type="integer"></property>
		<property name="teId" column="te_id" type="integer"></property>

	</class>

</hibernate-mapping>
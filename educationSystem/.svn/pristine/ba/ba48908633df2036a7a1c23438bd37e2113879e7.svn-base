<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.icss.entity.Grade" table="grade">
		<id name="grId" column="gr_id" type="integer">
			<generator class="native"></generator>
		</id>
		<!-- 普通属性 -->
		<property name="stNum" column="st_num" type="string"></property>
		<property name="coName" column="co_name" type="string"></property>
		<property name="score" column="score" type="double"></property>
	   
	</class>

</hibernate-mapping>
<script type="text/javascript">
/* 获取选中单选框 */
function testradio(){
	var v = $(":radio:checked").val();
	if(v!=undefined){
		$("#teMethod").val(v);
		$("#subform").submit();
}else{
	alert("您还没有选择录入种类");
}
	
	 
	
	/*  alert(v);  */
}
</script>
